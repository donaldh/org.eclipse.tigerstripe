<plugin>
   <extension-point id="packageFinder" name="packageFinder" schema="schema/packageFinder.exsd"/>
   <extension-point id="resourceProcessor" name="resourceProcessor" schema="schema/resourceProcessor.exsd"/>
   <extension-point id="listener" name="Resource Listener" schema="schema/listener.exsd"/>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http:///org/eclipse/tigerstripe/espace/resources.ecore" 
       class = "org.eclipse.tigerstripe.espace.resources.ResourcesPackage"
       genModel = "model/resources.genmodel" /> 
  </extension>
  
  <extension
        point="org.eclipse.emf.ecore.extension_parser">
     <parser
           class="org.eclipse.tigerstripe.espace.resources.format.AnnotationResourceFactory"
           type="ann"/>
  </extension>

</plugin>
