## Must have this element and it's not nillable
#set ($limits = 'minOccurs="1" maxOccurs="1" nillable="false"')
#set ($fName = 'return')
#if (!$method.isVoid())
#if ($method.ReturnType.isEnum())
#if ($method.ReturnType.isExtensibleEnum())
## NB : This will have a "matched pair" outside the complexType Def..
## This is horrible......but it's in the spec
 				<element ref="$schemaUtils.getPrefixForArtifact($method.ContainingArtifact.FullyQualifiedName):base$util.capitalize($fName)" minOccurs="0"/>
#else
    			<element name="${fName}" type="$ossjXmlUtil.typeNSOf($schemaUtils.getPrefixForArtifact($method.ReturnType.FullyQualifiedName),$method.ReturnType.FullyQualifiedName, "").Name" $limits />
#end
#elseif ($method.ReturnType.isEntityType())
#if ($method.isReturnRefByValue())
                <element name="${fName}" type="$ossjXmlUtil.typeNSOf($schemaUtils.getPrefixForArtifact($method.ReturnType.FullyQualifiedName),$method.ReturnType.FullyQualifiedName, $method.ReturnType.Dimensions).Name" $limits />
#elseif ($method.isReturnRefByKey())
                <element name="${fName}" type="$ossjXmlUtil.typeNSOf($schemaUtils.getPrefixForArtifact($method.ReturnType.FullyQualifiedName),$method.ReturnType.FullyQualifiedName, $method.ReturnType.Dimensions).KeyName" $limits />
#elseif ($method.isReturnRefByKeyResult())
                <element name="${fName}" type="$ossjXmlUtil.typeNSOf($schemaUtils.getPrefixForArtifact($method.ReturnType.FullyQualifiedName),$method.ReturnType.FullyQualifiedName, $method.ReturnType.Dimensions).KeyResultName" $limits />
#end
#elseif ($method.ReturnType.isArtifact())
				<element name="${fName}" type="$ossjXmlUtil.typeNSOf($schemaUtils.getPrefixForArtifact($method.ReturnType.FullyQualifiedName),$method.ReturnType.FullyQualifiedName, $method.ReturnType.Dimensions).Name" $limits />
#else
#set ($thisType = $ossjXmlUtil.typeNSOf($schemaUtils.getPrefixForArtifact($method.ReturnType.FullyQualifiedName), $method.ReturnType.FullyQualifiedName, $method.ReturnType.Dimensions).Name)
                <element name="${fName}" type="$thisType" $limits />
#end
#end