$artifactType:METHOD,$object.FullyQualifiedName,$method.Name,$method.Optional,$method.Void,$method.ReturnType.FullyQualifiedName,$method.ReturnType.Multiplicity,$method.ReturnRefByString,$method.IteratorReturn,$method.Visibility,"$method.Comment"
#set ($position = 0)
#foreach ($argument in $method.Arguments)
$artifactType:METHOD:ARGUMENT,$object.FullyQualifiedName,$method.Name,$position,$argument.Name,$argument.Type.FullyQualifiedName,$argument.Type.Multiplicity,$argument.RefByString
#set ($position = $position + 1)
#end
#foreach ($exception in $method.exceptions)
$artifactType:METHOD:EXCEPTION,$object.FullyQualifiedName,$method.Name,$exception.FullyQualifiedName
#end
#end
## OPTIONALLY ADD INHERITED METHODS
#if ($includeInherited == "true")
#foreach ($method in $object.InheritedMethods)
$artifactType:METHOD,$object.FullyQualifiedName,$method.Name,$method.Optional,$method.Void,$method.ReturnType.FullyQualifiedName,$method.ReturnType.Multiplicity,$method.ReturnRefByString,$method.IteratorReturn,$method.Visibility,"$method.Comment",$method.ContainingArtifact.FullyQualifiedName
#foreach ($argument in $method.Arguments)
$artifactType:METHOD:ARGUMENT,$object.FullyQualifiedName,$method.Name,$position,$argument.Name,$argument.Type.FullyQualifiedName,$argument.Type.Multiplicity,$argument.RefByString
#set ($position = $position + 1)
#end
#foreach ($exception in $method.exceptions)
$artifactType:METHOD:EXCEPTION,$object.FullyQualifiedName,$method.Name,$exception.FullyQualifiedName
#end
#end
#end