#if ($fieldRestriction == "true" )
#set($limits = 'nillable="true" minOccurs="0"')
#else
#set ($limits = "")
#end
#set ($fName = $field.Name)
#if ($field.Type.isEnum())
#if ($field.Type.isExtensibleEnum())
## NB : This will have a "matched pair" outside the complexType Def..
## This will be in the local namespace
 					<element ref="$schemaUtils.getPrefixForArtifact($method.ContainingArtifact.FullyQualifiedName):base$util.capitalize($field.Name)" minOccurs="0"/>
#else
    				<element name="$fName" type="$ossjXmlUtil.typeNSOf($schemaUtils.getPrefixForArtifact($field.Type.FullyQualifiedName),$field.Type.FullyQualifiedName, "").Name" $limits />
#end
#elseif ($field.Type.isDataType())
					<element name="$fName" type="$ossjXmlUtil.typeNSOf($schemaUtils.getPrefixForArtifact($field.Type.FullyQualifiedName),$ossjUtil.interfaceOf($field.Type.FullyQualifiedName), $field.Type.Dimensions).Name" $limits />
#elseif ($field.Type.isEntityType())
#if ($field.isRefByValue())
                    <element name="${fName}" type="$ossjXmlUtil.typeNSOf($schemaUtils.getPrefixForArtifact($field.Type.FullyQualifiedName),$field.Type.FullyQualifiedName, $field.Type.Dimensions).Name" $limits />
#elseif ($field.isRefByKey())
                    <element name="${fName}" type="$ossjXmlUtil.typeNSOf($schemaUtils.getPrefixForArtifact($field.Type.FullyQualifiedName),$field.Type.FullyQualifiedName, $field.Type.Dimensions).KeyName" $limits />
#elseif ($field.isRefByKeyResult())
                    <element name="${fName}" type="$ossjXmlUtil.typeNSOf($schemaUtils.getPrefixForArtifact($field.Type.FullyQualifiedName),$field.Type.FullyQualifiedName, $field.Type.Dimensions).KeyResultName" $limits />
#end
#else
#set ($thisType = $ossjXmlUtil.typeNSOf($schemaUtils.getPrefixForArtifact($field.Type.FullyQualifiedName), $field.Type.FullyQualifiedName, $field.Type.Dimensions).Name)
                    <element name="$fName" type="$thisType" $limits />
#end