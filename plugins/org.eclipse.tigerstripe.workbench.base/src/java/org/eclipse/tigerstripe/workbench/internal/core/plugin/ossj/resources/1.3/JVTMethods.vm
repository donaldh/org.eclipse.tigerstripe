        $comma $ossjUtil.keyResultInterfaceOf($argument.Type.FullyQualifiedName).FullyQualifiedName$argument.Type.Dimensions $argument.Name
#end
#elseif ($argument.Type.isEnum())
        $comma $argument.Type.BaseType.FullyQualifiedName$argument.Type.Dimensions $argument.Name
#else
