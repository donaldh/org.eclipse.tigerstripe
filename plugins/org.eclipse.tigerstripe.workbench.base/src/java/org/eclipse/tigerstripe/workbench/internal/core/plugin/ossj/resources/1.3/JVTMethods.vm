#end
#end
## Set the @return
#if (! $method.isVoid())
 * @return $return
#end
## Set the @param
#foreach ($argument in $method.Arguments)
 * @param $argument.Name - $argument.Comment
#end
 *
#foreach($exception in $method.Exceptions)
#if ($ossjUtil.isTSArtifact($exception.FullyQualifiedName))
 * @throws $ossjUtil.interfaceOf($exception.FullyQualifiedName).FullyQualifiedName
#else
