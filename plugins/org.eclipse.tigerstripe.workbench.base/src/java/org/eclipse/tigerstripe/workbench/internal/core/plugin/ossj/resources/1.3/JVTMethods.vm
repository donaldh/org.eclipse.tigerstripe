#end
#if ($method.isOptional())
 * @throws javax.oss.OssUnsupportedAttributeException
#end	
#end
 */
    public $return ${method.Name} (
#set( $comma = " " )
#foreach ($argument in $method.Arguments)
#if ($argument.Type.isDatatype() || $argument.Type.isQueryType() || $argument.Type.isUpdateProcedureType() )
