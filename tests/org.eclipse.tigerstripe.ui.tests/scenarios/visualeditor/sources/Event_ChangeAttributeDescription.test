Entry-Name: .content
Content-Type: text/ecl

get-view "Tigerstripe Explorer" | get-tree | select "test/src/com.mycompany/Event0" | double-click
with [get-editor Event0] {
    with [get-section Attributes] {
        get-section | get-button Add | click
        with [get-section -index 1 | get-editbox -after [get-label "Description:"]] {
            set-text qqq
            key-type "M1+s"
        }
    }
    get-tab-folder | get-tab-item Source | click
}
get-editor Event0 | get-editbox | get-property text | contains qqq | verify-true
get-eclipse-window | key-type "TRAVERSE_ESCAPE"
get-eclipse-window | key-type ESC
with [get-editor Event0] {
    with [get-editbox] {
        set-text-offset 10 33
        key-type BackSpace
        key-type BackSpace
        key-type BackSpace
        type-text www
        key-type "M1+s"
    }
    get-tab-folder | get-tab-item Overview | click
}
get-editor Event0 | get-section Attributes | get-section -index 1 | get-editbox -after [get-label "Description:"] 
    | get-property text | contains www | verify-true
