get-view "Tigerstripe Explorer" | get-tree | select "test/src/com.mycompany/Association0" | double-click
with [get-editor Association0] {
    get-section Stereotypes | get-section -index 1 | mouse-release 240 35 button1 524288
    with [get-section -after [get-label "Association Artifact"] -index 5 | get-section "aEnd Details" 
        | get-button isNavigable] {
        click
        key-type "M1+s"
    }
    get-tab-folder | get-tab-item Source | click
}
get-editor Association0 | get-editbox | get-property text | contains "isNavigable = \"true\"" | verify-true
with [get-editor Association0] {
    with [get-editbox] {
        set-text-offset 22 34
        key-type Del
        key-type Del
        key-type Del
        key-type Del
        type-text false
        key-type "M1+s"
    }
    get-tab-folder | get-tab-item Overview | click
}
get-editor Association0 | get-section -after [get-label "Association Artifact"] -index 5 | get-section "aEnd Details" 
    | get-button isNavigable | get-property selected | equals false | verify-true
