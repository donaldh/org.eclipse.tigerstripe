Content-Type: text/ecl

get-view "Tigerstripe Explorer" | get-tree | select "test/src/com.mycompany/Datatype1" | double-click
with [get-editor Datatype1 | get-section Methods] {
    click
    get-section | get-button Add | click
    get-section -index 1 | get-button Add -after [get-label Exceptions] | click
}
get-window "Exception Selection for Method" | get-table | select "com.mycompany.Exception0" | click -default
with [get-editor Datatype1] {
    get-section Methods | get-section -index 1 | get-button Add -after [get-label Exceptions] | key-type "M1+s"
    get-tab-folder | get-tab-item Source | click
}
get-editor Datatype1 | get-editbox | get-property text | contains "com.mycompany.Exception0" | verify-true
with [get-editor Datatype1] {
    with [get-editbox] {
        set-text-offset 39 62
        key-type Del
        key-type "NUMPAD_5" 5
        key-type "M1+s"
    }
    get-tab-folder | get-tab-item Overview | click
}
get-editor Datatype1 | get-section Methods | get-section -index 1 | get-table -after [get-label Exceptions] 
    | get-item "com.mycompany.Exception5" | get-property caption | equals "com.mycompany.Exception5" | verify-true
