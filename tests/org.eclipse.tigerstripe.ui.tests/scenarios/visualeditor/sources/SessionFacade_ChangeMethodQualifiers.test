Entry-Name: .content
Content-Type: text/ecl

get-view "Tigerstripe Explorer" | get-tree | select "test/src/com.mycompany/SessionFacade0" | double-click
with [get-editor SessionFacade0] {
    with [get-section Methods] {
        get-section | get-button Add | click
        with [get-section -index 1 | get-button Abstract] {
            click
            key-type "M1+s"
        }
    }
    get-tab-folder | get-tab-item Source | click
}
get-editor SessionFacade0 | get-editbox | get-property text | contains "isAbstract = \"true\"" | verify-true
with [get-editor SessionFacade0] {
    with [get-editbox] {
        set-text-offset 19 38
        key-type Del
        key-type Del
        key-type Del
        key-type Del
        type-text false
        key-type "M1+s"
    }
    get-tab-folder | get-tab-item Overview | click
}
get-editor SessionFacade0 | get-section Methods | get-section -index 1 | get-button Abstract | get-property selected 
    | equals false | verify-true
