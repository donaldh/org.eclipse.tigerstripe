------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Tigerstripe Explorer" | get-tree | select "MyProject/MyProfile.wbp" 
    | get-menu "Profile/Set as active profile." | click
get-window "Save as Active Profile" | get-button OK | click
//wait-until-eclipse-is-ready
get-preferences-menu | click 
with [get-window Preferences] {
    get-tree | select "Tigerstripe/Explorer"
    get-group "Display Stereotypes and Annotations" | get-button "on Methods" | click
    get-button Apply | click
    get-button OK | click
}
get-view "Tigerstripe Explorer" | get-tree 
    | get-item "MyProject/src/com.mycompany/Datatype1/<<MyStereotype>>method0\\(String, String\\):void" 
    | get-property caption | equals "<<MyStereotype>>method0(String, String):void" | verify-true
