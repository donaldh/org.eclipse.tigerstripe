------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Tigerstripe Explorer" | get-tree | select "ProjectWithReference/default2.wfc" 
    | get-menu "Facets/Mark as Active" | click
get-view "Tigerstripe Explorer" | get-tree | get-item "ProjectWithReference \\{default2\\}" | get-property styleRanges 
    -index 0 | equals "0, 31, fontStyle=normal, foreground=Color {232, 123, 20}" | verify-true
get-view "Tigerstripe Explorer" | get-tree 
    | get-item "ProjectWithReference \\{default2\\}/Model References/New Project/src/org.eclipse.Package0/Entity3" 
    | get-property styleRanges -index 0 | equals "0, 7, fontStyle=normal, foreground=Color {0, 0, 0}" 
    | verify-true
get-view "Tigerstripe Explorer" | get-tree 
    | select "ProjectWithReference \\{default2\\}/Model References/New Project/src/org.eclipse"
with [get-view "Tigerstripe Explorer" | get-tree] {
    get-item "ProjectWithReference \\{default2\\}/Model References/New Project/src/org.eclipse/AssocClass0" 
        | get-property styleRanges -index 0 | equals "0, 11, fontStyle=normal, foreground=Color {128, 128, 128}" 
        | verify-true
