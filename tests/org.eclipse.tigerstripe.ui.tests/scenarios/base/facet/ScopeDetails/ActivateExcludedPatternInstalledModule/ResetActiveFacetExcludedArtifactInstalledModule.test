Content-Type: text/ecl

get-view "Tigerstripe Explorer" | get-tree | select "Project1/default.wfc" | get-menu "Facets/Mark as Active" | click
get-view "Tigerstripe Explorer" | get-tree 
    | get-item "Project1 {default}/Model References/org.eclipse.tigerstripe.test.module_1.0.0.jar - .*plugins/org.eclipse.tigerstripe.test/E1" 
    | get-property styleRanges -index 0 | equals "0, 2, fontStyle=normal, foreground=Color {128, 128, 128}" 
    | verify-true
get-view "Tigerstripe Explorer" | get-tree | select "Project1 \\{default\\}/default.wfc" 
    | get-menu "Facets/Reset Active Facet" | click
with [get-view "Tigerstripe Explorer" | get-tree] {
    get-item Project1 | get-property caption | equals Project1 | verify-true
    get-item "Project1/Model References/org.eclipse.tigerstripe.test.module_1.0.0.jar - .*plugins/org.eclipse.tigerstripe.test/Association1" 
