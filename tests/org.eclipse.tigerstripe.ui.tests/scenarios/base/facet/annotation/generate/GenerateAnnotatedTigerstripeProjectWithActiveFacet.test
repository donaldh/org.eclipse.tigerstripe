Project Generation with Active Facets with annotation in Tigerstripe project 

Precondition
GeneratorAndProjectAnnotationScopeFacetWorkspaceContext
AnnotationsContextPreferences

Steps
Mark facet as active
Deploy generator
Enable generator in TS project
Run generation
Verify created artifacts in target folder
Verify that inactive artifacts are not generated
------=_.description-216f885c-d591-38ce-8ea2-e4f8cb4d6ffa--
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

with [get-view "Tigerstripe Explorer" | get-tree] {
    select "New Project/default.wfc" | get-menu "Facets/Mark as Active" | click
    select "New Generator Project/ts-plugin.xml" | double-click -nowait
}
get-editor "New Generator Project/ts-plugin.xml" | get-section Testing | get-link "Package and deploy this generator" 
    | click
get-window "Deploy new generator" | get-button OK | click
get-window "New Generator Project Generator" | get-button OK | click
get-view "Tigerstripe Explorer" | get-tree | select "New Project \\{default\\}/tigerstripe.xml" | double-click -nowait
with [get-editor "New Project/tigerstripe.xml"] {
    get-tab-folder | get-tab-item "Generator Settings" | click
    with [get-section "New Generator Project(3.0) (disabled) [Generic]"] {
        click
        get-button "Enable Generator" | click
    }
}
get-button "Save.*" | click
get-view "Tigerstripe Explorer" | get-tree | select "New Project \\{default\\}" | get-menu "Generate..." | click
with [get-window "Generate Tigerstripe Project"] {
    get-button Finish | click
    get-window "Generation Result" | get-button OK | click
}
get-view "Tigerstripe Explorer" | get-tree | select "New Project \\{default\\}/target/tigerstripe.gen/AssocClass0.txt"
get-view "Tigerstripe Explorer" | get-tree | get-item "New Project \\{default\\}/target/tigerstripe.gen/AssocClass0.txt" 
    | get-property caption | equals "AssocClass0.txt" | verify-true
with [get-view "Tigerstripe Explorer" | get-tree] {
    get-item "New Project \\{default\\}/target/tigerstripe.gen/AssocClass0.txt" | get-property caption 
        | equals "AssocClass0.txt" | verify-true
    get-item "New Project \\{default\\}/target/tigerstripe.gen/Association0.txt" | get-property caption 
        | equals "Association0.txt" | verify-true
    get-item "New Project \\{default\\}/target/tigerstripe.gen/Entity0.txt" | get-property caption | equals "Entity0.txt" 
        | verify-true
    get-item "New Project \\{default\\}/target/tigerstripe.gen/Entity1.txt" | get-property caption | equals "Entity1.txt" 
        | verify-true
    verify-error {get-item "New Project \\{default\\}/target/tigerstripe.gen/Datatype0.txt"}
    verify-error {get-item "New Project \\{default\\}/target/tigerstripe.gen/Dependency0.txt"}
    verify-error {get-item "New Project \\{default\\}/target/tigerstripe.gen/Enumeration0.txt"}
    verify-error {get-item "New Project \\{default\\}/target/tigerstripe.gen/Event0.txt"}
    verify-error {get-item "New Project \\{default\\}/target/tigerstripe.gen/Exception0.txt"}
    verify-error {get-item "New Project \\{default\\}/target/tigerstripe.gen/Query0.txt"}
    verify-error {get-item "New Project \\{default\\}/target/tigerstripe.gen/Session0.txt"}
    verify-error {get-item "New Project \\{default\\}/target/tigerstripe.gen/Update0.txt"}
}
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
