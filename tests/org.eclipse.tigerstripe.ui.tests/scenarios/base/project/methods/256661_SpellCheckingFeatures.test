<com.xored.q7.scenario:Scenario xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:com.xored.q7.scenario="http://com/xored/q7/scenario.ecore" xmlns:com.xored.tesla.core.protocol="http:///com/xored/tesla/core/protocol.ecore" xmlns:com.xored.tesla.core.protocol.raw="http:///com/xored/tesla/core/protocol/raw.ecore" xmlns:com.xored.tesla.core.ui="http:///com/xored/tesla/core/ui.ecore" xmlns:org.eclipse.ecl.core="http://www.eclipse.org/ecl/core.ecore" name="256661_SpellCheckingFeatures" version="3.0" id="_zIO_QI24EeCQJ7V2A3r4wA" description="Check spell-checking functionality within artifact's Description field in TS editors &#xD;&#xA;&#xD;&#xA;Precondition&#xD;&#xA;ArtifactsContext&#xD;&#xA;&#xD;&#xA;Steps:&#xD;&#xA;    Open Preferences/General/Editors/Text Editors/Spelling&#xD;&#xA;    Check enabled checkbox &quot;Enable spell checking&quot;&#xD;&#xA;    Add not correctly spelled word in artifact's Description field&#xD;&#xA;    Select correctly spelled word in dropdown menu and update word&#xD;&#xA;    Disable spell checking in dropdown menu&#xD;&#xA;    Check disabled checkbox &quot;Enable spell checking&quot; in Preferences/General/Editors/Text Editors/Spelling&#xD;&#xA;" tags="TS_Project/Edit/Artifacts, I35" type="ecl" externalReference="http://docs.xored.com/display/TS/256661_SpellCheckingFeatures">
  <contexts>_aOd6IKw8Ed-cSZ2bfD-mDg</contexts>
  <content xsi:type="org.eclipse.ecl.core:Script">
    <content><![CDATA[get-preferences-menu | click
get-window Preferences | get-tree | select "General/Editors/Text Editors/Spelling"
get-window Preferences | get-button "Enable spell checking" | get-property selected | equals true | verify-true
get-window Preferences | get-button OK | click
get-view "Tigerstripe Explorer" | get-tree | select "New Project/src/org.eclipse/Entity0" | double-click -nowait
with [get-editor Entity0 | get-section "General Information" | get-editbox -after [get-label "Description:"]] {
    type-text Creat
}
with [get-editor Entity0 | get-section "General Information" | get-editbox -after [get-label "Description:"]] {
    set-text-offset 2 0
    get-menu "Change to \'Create\'" | click
}
get-editor Entity0 | get-section "General Information" | get-editbox -after [get-label "Description:"] 
    | get-property rawValue | equals Create | verify-true
with [get-editor Entity0 | get-section "General Information" | get-editbox -after [get-label "Description:"]] {
    set-text-offset 6 0
    key-type BS
    get-menu "Disable spell checking" | click
}
get-button "Save (Ctrl+S)" | click
get-editor Entity0 | get-section "General Information" | get-editbox -after [get-label "Description:"] 
    | set-text-offset 2 0
get-preferences-menu | click
get-window Preferences | get-tree | select "General/Editors/Text Editors/Spelling"
get-window Preferences | get-button OK | click]]></content>
  </content>
