------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Tigerstripe Explorer" | get-tree | get-menu "New/Tigerstripe Project" | click
with [get-window "New Tigerstripe Project"] {
    get-editbox -after [get-label "Project Name:"] | set-text Project1
    get-button Finish | click
}
get-view "Tigerstripe Explorer" | get-tree | get-menu "New/Tigerstripe Project" | click
get-window "New Tigerstripe Project" | get-editbox -after [get-label "Project Name:"] | set-text project1
exec-with-options {
    get-window "New Tigerstripe Project" | get-button Finish | click
    get-window "New Tigerstripe Project" | get-window "Problem Occurred" 
        | get-label "The underlying file system is case insensitive. There is an existing project or directory that conflicts with \'project1\'." 
        | get-property caption 
        | equals "The underlying file system is case insensitive. There is an existing project or directory that conflicts with \'project1\'." 
        | verify-true
    get-window "New Tigerstripe Project" | get-window "Problem Occurred" | get-button OK | click
} -allowStatusDialog
get-window "New Tigerstripe Project" | get-button Cancel | click
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
