------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Tigerstripe Explorer" | get-tree | select "New Project/src" | get-menu "New/Event" | click
with [get-window "Create a new Event"] {
    get-editbox -after [get-label "Name:"] | set-text Event0
    get-editbox -after [get-label "Artifact Package:"] | get-property value | is-equal "org.eclipse" | verify
    get-button Finish | click
}
with [get-editor Event0] {
    click
    with [get-section "General Information"] {
        get-editbox -after [get-label "Project:"] | get-property value | is-equal "New Project" | verify
        get-editbox -after [get-label "Qualified Name:"] | get-property value | is-equal "org.eclipse.Event0" | verify
        get-editbox -after [get-label "Description:"] | get-property value | is-equal "" | verify
        get-editbox -after [get-label "Description:"] -index 1 | get-property value | is-equal "" | verify
    }
    get-canvas | mouse-move 183 32
    get-section Details | get-canvas -after [get-label Details] -index 1 | mouse-move 318 19
    with [get-section Attributes] {
        get-button Add | click
        with [get-editbox -after [get-label "Name:"]] {
            key-type "Ctrl+a"
            set-text "event0_testField0"
        }
    }
}
get-button "Save \(.*\)" | click
with [get-editor Event0] {
    with [get-section Attributes] {
        get-table -after [get-label Attributes] | get-item "event0_testField0" | get-property columns -index 0 | is-equal "event0_testField0" | verify
        get-editbox -after [get-label "Type:"] | get-property value | is-equal String | verify
        get-button Public | get-property selected | is-equal true | verify
        get-button Readonly | get-property selected | is-equal false | verify
    }
    with [get-section Constants] {
        click
        get-button Add | click
        with [get-editbox -after [get-label "Name:"]] {
            key-type "Ctrl+a"
            set-text "event0_testLiteral0"
        }
    }
}
get-button "Save \(.*\)" | click
with [get-editor Event0] {
    get-section Constants | get-table -after [get-label Constants] | get-item "event0_testLiteral0=\"0\"" | get-property columns -index 0 | is-equal "event0_testLiteral0=\"0\"" | verify
    with [get-section Details] {
        get-canvas | mouse-move 314 29
        get-canvas -after [get-label Details] -index 1 | mouse-move 106 24
    }
}
with [get-view "Tigerstripe Explorer"] {
    click
    with [get-tree] {
        select "New Project/src/org.eclipse" | get-item "New Project/src/org.eclipse/Event0" | get-property caption | is-equal Event0 | verify
        get-item "New Project/src/org.eclipse/Event0/event0_testField0:String" | get-property caption | is-equal "event0_testField0:String" | verify
        get-item "New Project/src/org.eclipse/Event0/event0_testLiteral0=\"0\"" | get-property caption | is-equal "event0_testLiteral0=\"0\"" | verify
    }
}
