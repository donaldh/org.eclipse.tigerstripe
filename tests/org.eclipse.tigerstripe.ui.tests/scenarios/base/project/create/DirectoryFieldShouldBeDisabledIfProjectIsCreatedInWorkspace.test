------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Tigerstripe Explorer" | get-tree | get-menu "New/Tigerstripe Project" | click
get-window "New Tigerstripe Project" | get-group Location | get-editbox -after [get-label "Directory:"] 
    | get-property enablement | equals false | verify-true
get-window "New Tigerstripe Project" | get-group Location | get-button "Create project at external location" | click
get-window "New Tigerstripe Project" | get-group Location | get-editbox -after [get-label "Directory:"] 
    | get-property enablement | equals true | verify-true
get-window "New Tigerstripe Project" | get-group Location | get-button "Create project in workspace" | click
get-window "New Tigerstripe Project" | get-group Location | get-editbox -after [get-label "Directory:"] 
    | get-property enablement | equals false | verify-true
