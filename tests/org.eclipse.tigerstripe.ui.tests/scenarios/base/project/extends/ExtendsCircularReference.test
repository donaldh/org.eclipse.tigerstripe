<com.xored.q7.scenario:Scenario xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:com.xored.q7.scenario="http://com/xored/q7/scenario.ecore" xmlns:com.xored.tesla.core.protocol="http:///com/xored/tesla/core/protocol.ecore" xmlns:com.xored.tesla.core.protocol.raw="http:///com/xored/tesla/core/protocol/raw.ecore" xmlns:com.xored.tesla.core.ui="http:///com/xored/tesla/core/ui.ecore" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:org.eclipse.ecl.core="http://www.eclipse.org/ecl/core.ecore" name="ExtendsCircularReference" version="3.0" id="_0D5zwaYWEeC0VacjaNx2WQ" description="Check error if circular reference between extended projects is created&#xD;&#xA;&#xD;&#xA;Precondition&#xD;&#xA;ExtendsContextGroup&#xD;&#xA;&#xD;&#xA;Steps:&#xD;&#xA;    Clear log viewer&#xD;&#xA;    Create extends to Entity2 for Entity1 artifact&#xD;&#xA;    Check messages in Problems view&#xD;&#xA;    Check empty error log&#xD;&#xA;    Remove extends to Entity2 for Entity1 artifact&#xD;&#xA;    Check empty Problems view&#xD;&#xA;" tags="TS_Project/Edit/Artifacts/Extends " type="ecl" externalReference="http://docs.xored.com/display/TS/ExtendsCircularReference">
  <contexts>_OPC4UOdkEd-SavNKxJeMNg</contexts>
  <content xsi:type="org.eclipse.ecl.core:Script">
    <content><![CDATA[with [get-view "Error Log"] {
    click -nowait
    get-button "Clear Log Viewer" | click
}
get-view "Tigerstripe Explorer" | get-tree | select "New Project/src/org.eclipse/Entity1" | double-click -nowait
get-editor Entity1 | get-section "General Information" | get-button Select | click
with [get-window "Super Entity"] {
    get-table | select "org.eclipse.Entity2"
    get-button OK | click
}
get-button "Save (Ctrl+S)" | click
get-view Problems | click -nowait
get-view Problems | get-tree 
    | get-item "Errors \\(2 items\\)/Cycle detected: a cycle exists in the type hierarchy between Entity1 and Entity2" 
    | get-property caption | equals "Cycle detected: a cycle exists in the type hierarchy between Entity1 and Entity2" 
    | verify-true
get-view Problems | get-tree | get-item "Errors \\(2 items\\)/The hierarchy of the type Entity2 is inconsistent" 
    | get-property caption | equals "The hierarchy of the type Entity2 is inconsistent" | verify-true
get-view "Error Log" | click -nowait
get-view "Error Log" | get-tree | get-property itemCount | equals 0 | verify-true
get-editor Entity1 | get-section "General Information" | get-button Select | click
with [get-window "Super Entity"] {
    get-table | select "<clear extension>"
    get-button OK | click
}
get-button "Save (Ctrl+S)" | click
get-view Problems | click -nowait
get-view Problems | get-tree | get-property itemCount | equals 0 | verify-true]]></content>
  </content>
