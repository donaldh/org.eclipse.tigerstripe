get-view "Tigerstripe Explorer" | get-tree | select "MyProject/default" | double-click
with [get-editor "default.wvd" | get-diagram | get-edit-part -className MapEditPart | get-edit-part -name En] {
    with [get-edit-part -text En] {
        mouse-move 98 11 button1 -height 16 -width 122
        mouse-press 98 11 button1 -height 16 -width 122
        mouse-release 98 11 button1 524288 -height 16 -width 122
        mouse-press 98 11 button1 -height 16 -width 122
        mouse-double-click 98 11 button1 -height 16 -width 122
    }
    get-edit-part -text "+attribute0:String" | get-edit-part -name attribute0 | mouse-release 93 10 button1 524288 
        -height 16 -width 112
}
with [get-editor En | get-section Attributes] {
    get-section | get-table | select attribute0
    get-section -index 1 | get-button Add | click
}
with [get-window "Stereotype Selection"] {
    get-table | select aStereotype0
    get-button OK | click
}
