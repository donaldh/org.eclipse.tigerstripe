------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Tigerstripe Explorer" | get-tree | select "MyProject/default.wbp" | double-click
get-editor "MyProject/default.wbp" | get-section "General Information" | get-link "Save as active profile and reload" 
    | click
get-window "Save as Active Profile" | get-button OK | click
wait-until-eclipse-is-ready
get-view "Tigerstripe Explorer" | get-tree | select "MyProject/default" | double-click
with [get-editor "default.wvd" | get-diagram | get-edit-part -className MapEditPart | get-edit-part -name En] {
    with [get-edit-part -text En] {
        mouse-move 89 10 button1 -height 16 -width 122
        mouse-press 89 10 button1 -height 16 -width 122
        mouse-release 89 10 button1 524288 -height 16 -width 122
        mouse-press 89 10 button1 -height 16 -width 122
        mouse-double-click 89 10 button1 -height 16 -width 122
    }
    get-edit-part -text "+attribute0:String" | get-edit-part -name attribute0 | mouse-release 84 9 button1 524288 
        -height 16 -width 112
}
with [get-editor En | get-section Methods] {
    click
    get-section | get-table | select "method0\\(\\):void"
    get-section -index 1 | get-button Add | click
}
with [get-window "Stereotype Selection"] {
    get-table | select aStereotype0
    get-button OK | click
}
get-editor En | get-section Methods | get-section -index 1 | get-table | key-type "M1+s"
with [get-editor "default.wvd"] {
    click
    with [get-diagram] {
        key-type "M1+s"
        get-edit-part -className MapEditPart | get-edit-part -name En | get-edit-part -text En | mouse-hover 74 4 
            -mask CTRL -height 16 -width 221
    }
}
get-editor "default.wvd" | get-diagram | get-edit-part -className MapEditPart | get-edit-part -name En | get-edit-part 
    -text "<<aStereotype0>> +method0():void" | get-edit-part -name method0 | get-property text 
    | equals "<<aStereotype0>> +method0():void" | verify-true
get-view "Tigerstripe Explorer" | get-tree | get-item "MyProject/src/com.mycompany/En/<<aStereotype0>>method0\\(\\):void" 
    | get-property caption | equals "<<aStereotype0>>method0():void" | verify-true
with [get-editor En] {
    click
    get-section Methods | get-section -index 1 | get-button Add | click
}
with [get-window "Stereotype Selection"] {
    get-table | select aStereotype1
    get-button OK | click
}
get-editor En | get-section Methods | get-section -index 1 | get-table | key-type "M1+s"
get-view "Tigerstripe Explorer" | get-tree 
    | get-item "MyProject/src/com.mycompany/En/<<aStereotype0,aStereotype1>>method0\\(\\):void" | get-property caption 
    | equals "<<aStereotype0,aStereotype1>>method0():void" | verify-true
get-editor "default.wvd" | click
get-editor "default.wvd" | get-diagram | get-edit-part -className MapEditPart | get-edit-part -name En | get-edit-part 
    -text "<<aStereotype0, aStereotype1>> +method0():void" | get-edit-part -name method0 | get-property text 
    | equals "<<aStereotype0, aStereotype1>> +method0():void" | verify-true
with [get-editor En] {
    click
    with [get-section Methods | get-section -index 1] {
        get-table | select aStereotype0
        get-button Remove | click
    }
}
get-window "Remove Stereotype" | get-button Yes | click
with [get-editor En | get-section Methods | get-section -index 1] {
    get-table | select aStereotype1
    get-button Remove | click
}
get-window "Remove Stereotype" | get-button Yes | click
get-editor En | get-section Methods | get-section -index 1 | get-button Add | key-type "M1+s"
get-view "Tigerstripe Explorer" | get-tree 
    | get-item "MyProject/src/com.mycompany/En/method0\\(\\):void" | get-property caption 
    | equals "method0():void" | verify-true
get-editor "default.wvd" | click
get-editor "default.wvd" | get-diagram | get-edit-part -className MapEditPart | get-edit-part -name En | get-edit-part 
    -text "+method0():void" | get-edit-part -name method0 | get-property text | equals "+method0():void" | verify-true
