------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Tigerstripe Explorer" | get-tree | select "MyProject/default.wbp" | double-click
get-editor "MyProject/default.wbp" | get-section "General Information" | get-link "Save as active profile and reload" 
    | click
get-window "Save as Active Profile" | get-button OK | click
//wait-until-eclipse-is-ready
get-view "Tigerstripe Explorer" | get-tree | select "MyProject/default" | double-click
get-view "Annotation Property View" | click
get-view "Tigerstripe Explorer" | get-tree | select "MyProject/src/com.mycompany/SF/method0\\(String\\):void"
get-view "Annotation Property View" | get-table | get-menu "Add Stereotype" | click
with [get-window "Stereotype Selection"] {
    get-table | select aStereotype0
    get-button OK | click
}
get-view "Tigerstripe Explorer" | get-tree 
    | get-item "MyProject/src/com.mycompany/SF/<<aStereotype0>>method0\\(String\\):void" | get-property caption 
    | equals "<<aStereotype0>>method0(String):void" | verify-true
get-editor "default.wvd" | get-diagram | get-edit-part -className MapEditPart | get-edit-part -name SF | get-edit-part 
    -text "<<aStereotype0>> +method0(String):void" | get-edit-part -name method0 | get-property text 
    | equals "<<aStereotype0>> +method0(String):void" | verify-true
get-view "Tigerstripe Explorer" | get-tree 
    | select "MyProject/src/com.mycompany/SF/<<aStereotype0>>method0\\(String\\):void"
get-view "Annotation Property View" | get-table | get-menu "Add Stereotype" | click
with [get-window "Stereotype Selection"] {
    get-table | select aStereotype1
    get-button OK | click
}
get-view "Tigerstripe Explorer" | get-tree 
    | get-item "MyProject/src/com.mycompany/SF/<<aStereotype0,aStereotype1>>method0\\(String\\):void" 
    | get-property caption | equals "<<aStereotype0,aStereotype1>>method0(String):void" | verify-true
get-editor "default.wvd" | get-diagram | get-edit-part -className MapEditPart | get-edit-part -name SF | get-edit-part 
    -text "<<aStereotype0, aStereotype1>> +method0(String):void" | get-edit-part -name method0 | get-property text 
    | equals "<<aStereotype0, aStereotype1>> +method0(String):void" | verify-true
get-view "Tigerstripe Explorer" | get-tree 
    | select "MyProject/src/com.mycompany/SF/<<aStereotype0,aStereotype1>>method0\\(String\\):void"
set-dialog-result MessageBox 64
get-view "Annotation Property View" | get-table | get-menu "Remove All" | click
get-view "Tigerstripe Explorer" | get-tree | get-item "MyProject/src/com.mycompany/SF/method0\\(String\\):void" 
    | get-property caption | equals "method0(String):void" | verify-true
get-editor "default.wvd" | get-diagram | get-edit-part -className MapEditPart | get-edit-part -name SF | get-edit-part 
    -text "+method0(String):void" | get-edit-part -name method0 | get-property text | equals "+method0(String):void" 
    | verify-true
