------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Tigerstripe Explorer" | get-tree | select "MyProject/default.wbp" | double-click
get-editor "MyProject/default.wbp" | get-section "General Information" | get-link "Save as active profile and reload" 
    | click
get-window "Save as Active Profile" | get-button OK | click
wait-until-eclipse-is-ready
get-view "Tigerstripe Explorer" | get-tree | select "MyProject/default" | double-click
with [get-editor "default.wvd" | get-diagram | get-edit-part -className MapEditPart | get-edit-part -name Upd] {
    with [get-edit-part -text Upd] {
        mouse-move 106 9 button1 -height 16 -width 122
        mouse-press 106 9 button1 -height 16 -width 122
        mouse-release 106 9 button1 524288 -height 16 -width 122
        mouse-press 106 9 button1 -height 16 -width 122
        mouse-double-click 106 9 button1 -height 16 -width 122
    }
    get-edit-part -text "+attribute0:String" | get-edit-part -name attribute0 | mouse-release 101 8 button1 524288 
        -height 16 -width 112
}
with [get-editor Upd | get-section Constants] {
    click
    get-section | get-table | select "literal0=\"0\""
    get-section -index 1 | get-button Add | click
}
with [get-window "Stereotype Selection"] {
    get-table | select aStereotype0
    get-button OK | click
}
get-editor Upd | get-section Constants | get-section -index 1 | get-table | key-type "M1+s"
get-view "Tigerstripe Explorer" | get-tree | get-item "MyProject/src/com.mycompany/Upd/<<aStereotype0>>literal0=\"0\"" 
    | get-property caption | equals "<<aStereotype0>>literal0=\"0\"" | verify-true
get-editor "default.wvd" | click
with [get-editor Upd] {
    click
    get-section Constants | get-section -index 1 | get-button Add | click
}
with [get-window "Stereotype Selection"] {
    get-table | select aStereotype1
    get-button OK | click
}
get-editor Upd | get-section Constants | get-section -index 1 | get-table | key-type "M1+s"
get-view "Tigerstripe Explorer" | get-tree 
    | get-item "MyProject/src/com.mycompany/Upd/<<aStereotype0,aStereotype1>>literal0=\"0\"" | get-property caption 
    | equals "<<aStereotype0,aStereotype1>>literal0=\"0\"" | verify-true
    
    
    get-view "Tigerstripe Explorer" | get-tree 
    | select "MyProject/src/com.mycompany/Upd/<<aStereotype0,aStereotype1>>literal0=\"0\"" | double-click
get-editor Upd | get-section Constants | get-section | get-table | select "literal0=\"0\""
    
with [get-editor Upd | get-section Constants | get-section -index 1] {
    get-table | select aStereotype0
    get-button Remove | click
}
get-window "Remove Stereotype" | get-button Yes | click
with [get-editor Upd | get-section Constants | get-section -index 1] {
    get-table | select aStereotype1
    get-button Remove | click
}

get-window "Remove Stereotype" | get-button Yes | click
get-editor Upd | get-section Constants | get-section | get-table | key-type "M1+s"
get-view "Tigerstripe Explorer" | get-tree | get-item "MyProject/src/com.mycompany/Upd/literal0=\"0\"" 
    | get-property caption | equals "literal0=\"0\"" | verify-true
