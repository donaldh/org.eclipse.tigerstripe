<com.xored.q7.scenario:Scenario xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:com.xored.q7.scenario="http://com/xored/q7/scenario.ecore" xmlns:com.xored.tesla.core.protocol="http:///com/xored/tesla/core/protocol.ecore" xmlns:com.xored.tesla.core.protocol.raw="http:///com/xored/tesla/core/protocol/raw.ecore" xmlns:com.xored.tesla.core.ui="http:///com/xored/tesla/core/ui.ecore" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:org.eclipse.ecl.core="http://www.eclipse.org/ecl/core.ecore" name="359336_DeleteReferencedProject" version="3.0" id="_4KVrUes7EeCRApZ7U5vdRw" description="java.lang.NullPointerException should not displayed when I delete referenced project&#xD;&#xA;&#xD;&#xA;Precondition&#xD;&#xA;SeveralProjectsContextGroup&#xD;&#xA;&#xD;&#xA;Steps&#xD;&#xA;1. Add project1 as reference to another project2&#xD;&#xA;2. Delete project1 from Tigerstripe explorer&#xD;&#xA;3. Remove project1 from dependencies of project2&#xD;&#xA;4. Save changes" tags="Descriptor, Dependency, I41" type="ecl" externalReference="http://docs.xored.com/display/TS/359336_DeleteReferencedProject">
  <contexts>_tc0j4H97EeCcSOu_oe8o1g</contexts>
  <content xsi:type="org.eclipse.ecl.core:Script">
    <content><![CDATA[get-view "Tigerstripe Explorer" | get-tree | select "Project1/tigerstripe.xml" | double-click -nowait
with [get-editor "Project1/tigerstripe.xml"] {
    get-tab-folder | get-tab-item Dependencies | click
    get-section Dependencies | get-button Add | click
}
with [get-window "Select Tigerstripe Project"] {
    get-table | select Project2
    get-button OK | click
}
get-button "Save (Ctrl+S)" | click
get-view "Tigerstripe Explorer" | get-tree | select Project2 | key-type DEL
with [get-window "Delete Resources"] {
    get-button "Delete project contents on disk (cannot be undone)" | click
    get-button OK | click
}
with [get-view Problems | get-tree | get-item "Errors \\(1 item\\)/Unresolved model reference with id \'Project2\'"] {
    get-property columns -index 0 | equals "Unresolved model reference with id \'Project2\'" | verify-true
    get-property columns -index 1 | equals "tigerstripe.xml" | verify-true
    get-property columns -index 2 | equals "/Project1" | verify-true
    get-property columns -index 3 | equals Unknown | verify-true
    get-property columns -index 4 | equals "Tigerstripe References Audit Marker" | verify-true
}
with [get-editor "Project1/tigerstripe.xml" | get-section Dependencies] {
    get-table | select Project2
    get-button Remove | click
}
get-window "Remove Reference/Dependency" | get-button Yes | click
get-button "Save (Ctrl+S)" | click
get-editor "Project1/tigerstripe.xml" | get-section Dependencies | get-table | get-property itemCount | equals 0 
    | verify-true]]></content>
  </content>
