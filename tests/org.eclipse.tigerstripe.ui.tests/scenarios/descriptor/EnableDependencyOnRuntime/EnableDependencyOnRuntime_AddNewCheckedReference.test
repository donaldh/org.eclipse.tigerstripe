------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Tigerstripe Explorer" | get-tree | select "Project1/tigerstripe.xml" | double-click -nowait
with [get-editor "Project1/tigerstripe.xml"] {
    get-tab-folder | get-tab-item Dependencies | click
    get-section Dependencies | get-button Add | click
}
with [get-window "Select Tigerstripe Project"] {
    get-table | select Project2
    get-button OK | click
}
with [get-editor "Project1/tigerstripe.xml" | get-section Dependencies | get-table] {
    get-item "Project2.*" | get-property caption | equals "Project2.*" | verify-true
    get-item "Project2.*" | get-property checked | equals true | verify-true
}
get-button "Save (Ctrl+S)" | click
get-editor "Project1/tigerstripe.xml" | get-section Dependencies | get-table | get-item "Project2.*" 
    | get-property checked | equals true | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
