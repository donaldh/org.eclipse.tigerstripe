    | select "New Project/src/org.eclipse/<<Font,Color,CheckAnnotation,IntegerAnnotation,TextAnnotation>>Entity0" 
    | get-menu Delete | click
get-window "Confirm Delete" | get-button OK | click
get-view "Tigerstripe Explorer" | get-tree | get-item "New Project/src/org.eclipse" | get-property childCount 
    | equals 1 | verify-true
get-view Problems | get-tree | get-property itemCount | equals 0 | verify-true]]></content>
