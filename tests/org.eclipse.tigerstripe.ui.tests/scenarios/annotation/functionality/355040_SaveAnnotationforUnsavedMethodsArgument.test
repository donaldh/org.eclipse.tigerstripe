  <content xsi:type="org.eclipse.ecl.core:Script">
    <content><![CDATA[get-view "Tigerstripe Explorer" | get-tree 
    | select "New Project/src/org.eclipse/<<Font,Color,CheckAnnotation,IntegerAnnotation,TextAnnotation>>Entity0/entity0_testMethod0\\(\\):void" 
    | double-click -nowait
get-editor Entity0 | get-section Methods | get-section -index 1 | get-button Add -after [get-label Arguments] | click
get-view "Annotation Property View" | click -nowait
get-editor Entity0 | get-section Methods | get-section -index 1 | get-table -after [get-label Arguments] 
    | select "arg0: String"
get-view "Annotation Property View" | get-table | get-menu "Add Annotation" | click
with [get-window "Create Annotation Wizard"] {
    get-tree | select "Primitive/Check Annotation"
    get-button Finish | click
}
get-button "Save (Ctrl+S)" | click
with [get-editor Entity0 | get-section Methods] {
    get-section | get-table | select "entity0_testMethod0\\(String\\):void"
    get-section -index 1 | get-table -after [get-label Arguments] | select "arg0: String"
}
get-view "Annotation Property View" | get-table | get-property itemCount | equals 1 | verify-true
get-view "Annotation Property View" | get-table | get-item CheckAnnotation | get-property caption 
    | equals CheckAnnotation | verify-true]]></content>
  </content>
