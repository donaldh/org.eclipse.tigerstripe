<com.xored.q7.scenario:Scenario xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:com.xored.q7.scenario="http://com/xored/q7/scenario.ecore" xmlns:com.xored.tesla.core.protocol="http:///com/xored/tesla/core/protocol.ecore" xmlns:com.xored.tesla.core.protocol.raw="http:///com/xored/tesla/core/protocol/raw.ecore" xmlns:com.xored.tesla.core.ui="http:///com/xored/tesla/core/ui.ecore" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:org.eclipse.ecl.core="http://www.eclipse.org/ecl/core.ecore" name="336882_EncodeStereotypeValues" version="3.0" id="_w1yC4JGGEeCHIbo4rEr7oQ" description="Check encoding of stereotype values &#xD;&#xA;&#xD;&#xA;Precondition:&#xD;&#xA;ProfileWithStereotypesContextGroup&#xD;&#xA;&#xD;&#xA;Steps:&#xD;&#xA;    Activate the profile&#xD;&#xA;    Create a new Datatype artifact&#xD;&#xA;    Add the newly created stereotype and add the following for its value:&#xD;&#xA;    a1 +a2&amp;>&lt;&quot;'testing...&#xD;&#xA;    Save the Datatype&#xD;&#xA;    Select the stereotype and click on Edit, notice the value is not changed&#xD;&#xA;" tags="TS_Project/Profile/Edit, I33" type="ecl" externalReference="http://docs.xored.com/display/TS/336882_EncodeStereotypeValues">
  <contexts>_yFRb4BGBEeC46s5Prxi4vQ</contexts>
  <content xsi:type="org.eclipse.ecl.core:Script">
    <content><![CDATA[get-view "Tigerstripe Explorer" | get-tree | select "Project1/default.wbp" | double-click -nowait
get-editor "Project1/default.wbp" | get-section "General Information" | get-link "Save as active profile and reload" 
    | click -nowait
get-window "Save as Active Profile" | get-button OK | click -nowait
wait-until-eclipse-is-ready
get-view "Tigerstripe Explorer" | get-tree | select "Project1/src/com.mycompany/Datatype" | double-click -nowait
get-editor Datatype | get-section Stereotypes | get-section | get-button Add | click
with [get-window "Stereotype Selection"] {
    get-table | select "attribute_stereo"
    get-button OK | click
}
get-window "Stereotype Details" | get-button OK | click
with [get-editor Datatype | get-section Stereotypes | get-section] {
    get-table | select "attribute_stereo"
    get-button Edit | click
}
with [get-window "Stereotype Details"] {
    with [get-editbox -after [get-label "string_attr"]] {
        key-type "M1+v"
        set-text "a1 +a2&><\"\'testing..."
    }
    get-button OK | click
}
get-button "Save (Ctrl+S)" | click
with [get-editor Datatype | get-section Stereotypes | get-section] {
    get-table | select "attribute_stereo"
    get-button Edit | click
}
get-window "Stereotype Details" | get-editbox -after [get-label "string_attr"] | get-property rawValue 
    | equals "a1 +a2&><\"\'testing..." | verify-true
get-window "Stereotype Details" | get-button OK | click
get-button "Save (Ctrl+S)" | click]]></content>
  </content>
