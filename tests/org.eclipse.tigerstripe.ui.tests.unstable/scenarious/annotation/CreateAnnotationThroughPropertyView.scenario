  <content xsi:type="org.eclipse.ecl.core:Script">
    <content><![CDATA[get-view "Tigerstripe Explorer" | get-tree | select "New Project"
get-view "Annotation Property View" | get-table | get-menu "Add Annotation" | click
with [get-window "Create Annotation Wizard"] {
    get-tree | select "Personality/Person"
    get-button Finish | click
}
get-window "Annotation Conflict" | get-button OK | click
get-view "Annotation Property View" | get-table | get-item Person | get-property caption | equals Person | verify-true
get-view "Tigerstripe Explorer" | get-tree | select "New Project"
return-from-os-dialog "MESSAGE_BOX" 64
get-view "Tigerstripe Explorer" | get-tree | get-menu "Annotations/Remove" | click]]></content>
  </content>
